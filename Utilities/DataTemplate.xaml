<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:AddressBook.View"
                    xmlns:vm="clr-namespace:AddressBook.ViewModel">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BookVM}">
        <view:Book/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddVM}">
        <view:Add/>
    </DataTemplate>
</ResourceDictionary>